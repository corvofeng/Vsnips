{
	"name": "Vsnips",
	"displayName": "Vsnips",
	"description": "Ultisnips for vscode",
	"icon": "images/icon.png",
	"publisher": "corvofeng",
	"version": "0.1.1",
	"engines": {
		"vscode": "^1.35.0"
	},
	"categories": [
		"Other"
	],
	"repository": "https://github.com/corvofeng/Vsnips",
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			}
		],
		"configuration": {
			"title": "Vsnips",
			"properties": {
				"Vsnips.SnipsDir": {
					"type": "array",
					"description": "Directories for snippets",
					"default": []
				},
				"Vsnips.VarFiles": {
					"type": "array",
					"description": "Files for snippets variables, we call it vimrc.",
					"default": []
				},
				"Vsnips.LogLevel": {
					"type": "string",
					"description": "The Vsnip's LogLevel",
					"default": "NO"
				}
			}
		},
		"languages": [
			{
				"id": "snippets",
				"extensions": [
					".snippets"
				],
				"aliases": [
					"snippets"
				]
			}
		],
		"grammars": [
			{
				"language": "snippets",
				"scopeName": "source.snippets",
				"path": "./syntaxes/snippets.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "yarn run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"@types/request": "^2.48.2",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28"
	},
	"dependencies": {
		"request": "^2.88.0",
		"js-logger": "^1.6.0"
	},
	"__metadata": {
		"id": "a42f9b10-ee80-433b-9043-fc3a48ac1b2e",
		"publisherDisplayName": "corvofeng",
		"publisherId": "a645e3ba-2d9a-4490-bad1-6b6ef6c089af"
	}
}
